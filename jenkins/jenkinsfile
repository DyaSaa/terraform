pipeline {
    agent any
    stages {
    stage('Terraform checkout') {
      steps {
        sh 'cd I:/'
        git branch: 'main', url: 'https://github.com/DyaSaa/terraform.git'
        
              
      }
    }
    
     stage('Cloned') {
      steps {
        
        sh 'echo " Source code cloned"'
                  
      }
    }
  }
}

